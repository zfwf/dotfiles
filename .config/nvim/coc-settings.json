{
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.files.defaultOptions": ["--number-select"],
  "list.source.files.excludePatterns": ["**/.git/**"],
  "list.source.mru.ignoreGitIgnore": true,
  "list.source.mru.defaultOptions": ["--number-select"],
  "coc.source.omni.filetypes": [
    "rust",
    "typescript",
    "python",
    "json",
    "jsonc"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "python",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "scala"
  ],
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.hoverTarget": "preview",
  "python.jediEnabled": false,
  "diagnostic.messageTarget": "float",
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  }
}
